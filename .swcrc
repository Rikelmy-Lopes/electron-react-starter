{
    "$schema": "https://json.schemastore.org/swcrc",
    "exclude": [
        "src/renderer"
    ],
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "dynamicImport": false,
            "decorators": false
        },
        "target": "es2020",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true
    },
    "minify": false,
    "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": true
    }
}